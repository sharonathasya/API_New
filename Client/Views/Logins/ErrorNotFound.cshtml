@{
    ViewData["Title"] = "Login";
    Layout = "_LayoutLogin";
}

<div class="container">
    <div class="text-center">
        <div class="error mx-auto text-white" data-text="404">401</div>
    </div>
</div>

@section scripts{
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
        Swal.fire({
            icon: 'error',
            title: 'Oops...',
            text: 'Page Not Found 404!'
        }).then(function () {
            window.location = "/Logins";
        });
    </script>
}
